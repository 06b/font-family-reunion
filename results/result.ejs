<table id="<%= slug %>">
	<thead>
		<th>Operating System</th>
		<th>Supported Font</th>
	</thead>
	<tbody>
		<% operatingSystems.forEach(function(os){ %>
			<tr class="<% if( os.support ) { %>supported<% } %><% if( os.alias ) { %> aliased<% } %><% if( os.unsupported ) { %> unsupported<% } %><% if( os.fallback ) { %> fallback<% } %>">
				<td class="<%= os.shortcode %>"><%= os.name %> <%= os.version %></td>
				<td><%= os.fontFamily %><% if( !os.fontFamily ) { %>&#160;<% } %></td>
			</tr>
		<% }) %>
	</tbody>
</table>

AddDefaultCharset utf-8 

RewriteEngine on
RewriteBase /

# Remove trailing slashes
RewriteRule ^([^/\.]+)/$ /$1 [R,L]

# OS lists
RewriteCond %{REQUEST_URI} !\.(js|ico|gif|jpg|png|css)$
RewriteRule ^os/([^/\.]+)$ index.php?os=$1 [L]

# Font Families
RewriteCond %{REQUEST_URI} !\.(js|ico|gif|jpg|png|css)$
RewriteRule ^([^/\.]+)$ index.php?families=$1 [L]